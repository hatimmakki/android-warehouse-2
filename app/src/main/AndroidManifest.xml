<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.maxwell.androidwarehouse2">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activities.DummyActivity"></activity>
        <activity android:name=".activities.ZoomInZoomOut"></activity>
        <activity android:name=".activities.VideoStreamingDemo"></activity>
        <activity android:name=".activities.VideoCaptureDemo"></activity>
        <activity android:name=".activities.TTS"></activity>
        <activity android:name=".activities.STT"></activity>
        <activity android:name=".activities.ShapesDemo"></activity>
        <activity android:name=".activities.MediaPlayerStreaming"></activity>
        <activity android:name=".activities.GlideDemo"></activity>
    </application>

</manifest>